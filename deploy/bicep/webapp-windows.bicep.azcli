env=dev
grp=az400-$env
loc=westeurope
bicep=webapp-windows.bicep

az group create -n $grp --location $loc

az deployment group what-if --resource-group $grp \
   --template-file $bicep \
   --parameters webAppName='bicep-site'

az deployment group create --resource-group $grp \
    --template-file $bicep \
    --parameters webAppName='bicep-site'